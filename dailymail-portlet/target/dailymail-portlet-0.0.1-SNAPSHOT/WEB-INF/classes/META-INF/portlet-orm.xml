<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.mir.news.model.impl.ArticleModelImpl">
		<attributes>
			<id name="articleId" />
			<basic name="authorId" />
			<basic name="name" />
			<basic name="text">
				<column name="TEXT_" />
			</basic>
			<basic name="status" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.mir.news.model.impl.ReviewModelImpl">
		<attributes>
			<id name="reviewId" />
			<basic name="reviewerId" />
			<basic name="name" />
			<basic name="text">
				<column name="TEXT_" />
			</basic>
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<entity class="com.mir.news.model.impl.ArticleImpl" name="Article">
		<table name="article" />
		<attributes />
	</entity>
	<entity class="com.mir.news.model.impl.ReviewImpl" name="Review">
		<table name="review" />
		<attributes />
	</entity>
</entity-mappings>